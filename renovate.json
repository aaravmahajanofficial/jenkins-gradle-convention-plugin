{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "automerge": true,
  "automergeType": "branch",
  "prConcurrentLimit": 1,
  "prHourlyLimit": 0,
  "packageRules": [
    {
      "groupName": "all-dependencies",
      "matchPackageNames": [
        "/.*/"
      ]
    }
  ]
}
